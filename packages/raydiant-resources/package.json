{
  "name": "raydiant-resources",
  "version": "4.1.6",
  "repository": "git@github.com:mirainc/raydiant-kit.git",
  "main": "lib/index.js",
  "scripts": {
    "build": "NODE_ENV=production BABEL_ENV=production babel src --out-dir lib",
    "test": "jest src --env=jsdom"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "parse-domain": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.42",
    "@babel/core": "7.0.0-beta.42",
    "babel-core": "^7.0.0-0",
    "babel-jest": "22.1.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-react-app": "4.0.0-next.66cc7a90",
    "jest": "^22.4.2"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "rootDir": "src",
    "setupFiles": [
      "<rootDir>/setupTests.js"
    ]
  }
}
